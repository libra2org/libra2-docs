{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "migrate": "cd scripts/migration && pnpm install && pnpm build && pnpm migrate",
    "postmigrate": "pnpm format:content",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:eslint": "eslint . --cache --cache-location=.astro/eslint/.cache",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:eslint": "pnpm run lint --fix",
    "format:prettier": "prettier . --write",
    "format:content": "remark \"src/content/**/*.mdx\" --output --use remark-mdx",
    "postinstall": "pnpm exec simple-git-hooks"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.2.0",
  "dependencies": {
    "@astrojs/markdown-remark": "^6.2.0",
    "@astrojs/node": "^9.1.2",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.2.1",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "^0.32.2",
    "@astrojs/starlight-docsearch": "^0.6.0",
    "@astrojs/starlight-tailwind": "^3.0.0",
    "@astrojs/tailwind": "^6.0.0",
    "@astrojs/vercel": "^8.1.1",
    "@expressive-code/plugin-collapsible-sections": "^0.40.2",
    "@graphiql/toolkit": "^0.11.1",
    "@iconify/json": "^2.2.318",
    "@nanostores/react": "^0.8.4",
    "@octokit/graphql": "^8.2.1",
    "@readme/openapi-parser": "^2.7.0",
    "@scalar/api-client": "^2.3.1",
    "@scalar/snippetz": "^0.2.14",
    "@tailwindcss/vite": "^4.0.6",
    "@types/gtag.js": "^0.0.20",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.6.5",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.5.2",
    "astro-breadcrumbs": "^3.3.1",
    "astro-embed": "^0.9.0",
    "class-variance-authority": "^0.7.1",
    "firebase": "^11.4.0",
    "graphiql": "^3.8.3",
    "gray-matter": "^4.0.3",
    "jose": "^6.0.7",
    "katex": "^0.16.21",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "nanostores": "^0.11.4",
    "octokit": "^4.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-math": "^6.0.0",
    "satori-html": "^0.3.2",
    "sharp": "^0.33.5",
    "shiki": "^2.3.2",
    "starlight-llms-txt": "^0.5.0",
    "starlight-openapi": "^0.13.0",
    "starlight-package-managers": "^0.10.0",
    "tailwindcss": "^4.0.6",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "unplugin-icons": "^22.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@crowdin/cli": "^4.6.1",
    "@eslint/js": "^9.20.0",
    "astro-eslint-parser": "^1.2.1",
    "eslint": "^9.20.1",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^15.15.0",
    "nano-staged": "^0.8.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.1",
    "prettier-plugin-astro": "^0.14.1",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.1.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.2.0"
  },
  "overrides": {
    "vite": "^6"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "@scalar/api-client": "patches/@scalar__api-client.patch",
      "starlight-llms-txt@0.5.0": "patches/starlight-llms-txt@0.5.0.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "astro check --noSync"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/content/**/*.mdx": [
      "remark --output --use remark-mdx"
    ]
  }
}
